<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".activity.act_viewWorkoutList"
            android:exported="false" />
        <activity
            android:name=".activity.act_viewWorkout"
            android:exported="false" />
        <activity
            android:name=".activity.act_challengeInfo"
            android:exported="false" />
        <activity
            android:name=".activity.act_bmiResult"
            android:exported="false" />
        <activity
            android:name=".activity.act_bmiCalculator"
            android:exported="false" />
        <activity
            android:name=".activity.act_SplashScreen"
            android:exported="false" />
        <activity
            android:name=".activity.act_dashBoard"
            android:exported="false" />
        <activity
            android:name=".activity.act_Login"
            android:exported="false" />
        <activity
            android:name=".activity.act_forgotPassword"
            android:exported="false" />
        <activity
            android:name=".activity.act_getStarted"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>